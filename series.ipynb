   "source": [
    "# program to read a number and print the series 1+2+3+.....+n=\n",
    "\n",
    "n = int(input(\"enter a number:\"))\n",
    "a = []\n",
    "for i in range(1, n + 1):\n",
    "    print(i, sep=\" \", end=\" \")\n",
    "    if i < n:\n",
    "        print(\"+\", sep=\" \", end=\" \")\n",
    "    a.append(i)\n",
    "print(\"=\", sum(a))\n",
    "print()"
   ]
