   "source": [
    "# find sum of digits n a number\n",
    "n = int(input(\"enter a number\"))\n",
    "s = 0\n",
    "while n != 0:\n",
    "    s = s + n % 10\n",
    "    n = n // 10\n",
    "print(\"sum :\", s)\n"
   ]
