    "# print an identity matrix\n",
    "n = int(input(\"enter a number:\"))\n",
    "for i in range(0, n):\n",
    "    for j in range(0, n):\n",
    "        if i == j:\n",
    "            print(\"1\", sep=\" \", end=\" \")\n",
    "        else:\n",
    "            print(\"0\", sep=\" \", end=\" \")\n",
    "    print()\n",
    "    \n",
    "\n"
   ]
